def process():
    client = f1ai_client.init(TOKEN)

    response = client.promote_model_registration(
        model_name="irisclassifier",
        model_version=1,
        model_source="databricks_registry"
    )

    # Cas 1 : si response est déjà un dict JSON
    if isinstance(response, dict):
        print("Response JSON:", response)
    # Cas 2 : si c'est un objet response de `requests`
    else:
        print("Status code:", response.status_code)
        print("Content-Type:", response.headers.get("Content-Type"))
        print("Raw body:", response.text[:500])  # Pour éviter d'afficher trop
